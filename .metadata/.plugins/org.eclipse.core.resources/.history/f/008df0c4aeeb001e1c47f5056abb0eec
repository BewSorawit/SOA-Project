package project.projectsoa.response;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.ToString;

@Data
@AllArgsConstructor
@NoArgsConstructor
@ToString
public class RentalResponse {
	private int rental_id;
	private String name;
	private String order_date;
	private String due_date;
	private String return_date;
    private String rent_status;
    private String payment_status;
    private String costume_name;
    
    public RentalResponse(int rental_id,String order_date,String due_date,String return_date,String rent_status,String payment_status,String costume_name)

}
